User Function MTA241CPO()
	Local aArea := GetArea()
	Local nOpc := ParamIXB[1]

	If nOpc == 3
		If !ExisteSX6("MV_DOCINTS")
			CriarSX6("MV_DOCINTS","C","Define o Sequencial da Movimentacao interna.","")
		EndIf

		cDocumento := StrZero(Val(GetMV("MV_DOCINTS"))+1,9)

		PutMV( "MV_DOCINTS", cDocumento )

	EndIf

	RestArea(aArea)
Return