RecLock(cArq,.F.)
If nEscolha == 1
   (cArq)->F2_DTLANC := Ctod("")
ElseIf nEscolha == 2            
   (cArq)->F1_DTLANC := Ctod("")
ElseIf nEscolha == 3
   (cArq)->E5_LA := ""
ElseIf nEscolha == 4   
   (cArq)->E1_LA := ""
ElseIf nEscolha == 5
   (cArq)->E2_LA := ""
EndIf 

MsUnlock()

Return


