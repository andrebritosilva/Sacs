User Function F430BXA()

_aArea := GetArea()

//If lBaixou .and. lMovAdto
	// Grava o Centro de Custo Debito e Credito no SE5 conforme o SE2 apos a baixa do titulo via Retorno Cobranca CNAB
	RecLock("SE5",.F.)
	SE5->E5_CCD := SE2->E2_CCD
	SE5->E5_CCC := SE2->E2_CCC
	MsUnLock()
//EndIf

RestArea(_aArea)

Return
