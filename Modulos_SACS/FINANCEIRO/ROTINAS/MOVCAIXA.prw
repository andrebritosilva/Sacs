If cTipo == "E"
   nSaldo += nValor 
Else               
   nSaldo -= nValor
EndIf 

RecLock("SZ2",.T.)
SZ2->Z2_FILIAL  := xFilial("SZ2")
SZ2->Z2_USER    := __cUserID
SZ2->Z2_NOMUSR  := UsrFullName(__cUserID)
SZ2->Z2_DATA    := dData
SZ2->Z2_TIPO    := cTipo
SZ2->Z2_VALOR   := nValor
SZ2->Z2_DESC    := cDesc
SZ2->Z2_SALDO   := nSaldo  
MsUnlock()

Return 

